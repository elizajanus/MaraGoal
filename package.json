  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
